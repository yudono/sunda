// Test Destructuring Parameters

println("=== Destructuring Test ===")

function test({ a, b, c }) {
    println("a = ", a)
    println("b = ", b)
    println("c = ", c)
}

var obj = { a: 10, b: 20, c: 30 }
test(obj)

println("")
println("=== Destructuring with Arrow Function ===")

var items = [
    { id: 1, name: "Apple" },
    { id: 2, name: "Banana" }
]

var names = items.map(({ name }) => {
    return name
})

println("Names: ", names)

println("")
println("=== All destructuring tests passed! ===")
