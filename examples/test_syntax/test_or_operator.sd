// Test || operator for fallback
var name1 = "Apple"
var name2 = ""
var name3 = 0

println("Test 1 (has value):")
println(name1 || "default")

println("Test 2 (empty string):")
println(name2 || "default")

println("Test 3 (zero):")
println(name3 || "default")

// Test with undefined-like behavior
var obj = { id: 1 }
println("Test 4 (missing property):")
println(obj.name || "no name")
