// Test Phase 1 Features

println("=== Logical AND (&&) ===")
var a = 1 && 2
println("1 && 2 = ", a)  // Should be 2

var b = 0 && 2
println("0 && 2 = ", b)  // Should be 0

var c = 5 && 10
println("5 && 10 = ", c)  // Should be 10

println("")
println("=== Logical OR (||) ===")
var d = 1 || 2
println("1 || 2 = ", d)  // Should be 1

var e = 0 || 2
println("0 || 2 = ", e)  // Should be 2

var f = 0 || 0
println("0 || 0 = ", f)  // Should be 0

println("")
println("=== Spread Operator ===")
var arr1 = [1, 2]
var arr2 = [...arr1, 3, 4]
println("arr1 = ", arr1)
println("[...arr1, 3, 4] = ", arr2)  // Should be [1, 2, 3, 4]

var arr3 = [10, 20, 30]
var arr4 = [0, ...arr3, 40]
println("[0, ...arr3, 40] = ", arr4)  // Should be [0, 10, 20, 30, 40]

println("")
println("=== All Phase 1 tests passed! ===")
