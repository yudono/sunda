var numbers = [4, 2, 7, 1, 3]

var i = 0
while (i < numbers.length) {
    var j = 0
    while (j < numbers.length - 1) {
        if (numbers[j] > numbers[j + 1]) {
            var t = numbers[j]
            numbers[j] = numbers[j + 1]
            numbers[j + 1] = t
        }
        j = j + 1
    }
    i = i + 1
}

println(numbers)
